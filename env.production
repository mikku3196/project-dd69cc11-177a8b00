# 自己進化型AIポートフォリオ自動売買システム - 環境設定
# Windows環境でのUTF-8対応

# Python UTF-8 エンコーディング設定
PYTHONUTF8=1

# ログレベル設定
LOG_LEVEL=INFO

# 環境設定
ENVIRONMENT=production

# データベース設定
DATABASE_URL=sqlite:///./trading_bot.db

# API設定（テストネット）
BYBIT_TESTNET=true
BYBIT_MODE=testnet

# システム設定
SYSTEM_NAME=自己進化型AIポートフォリオ自動売買システム
SYSTEM_VERSION=1.0.0
